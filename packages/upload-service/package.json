{
  "name": "@ar-io-bundler/upload-service",
  "version": "1.0.0",
  "main": "index.js",
  "packageManager": "yarn@3.2.1",
  "type": "commonjs",
  "scripts": {
    "clean": "rimraf [ lib .nyc_output coverage ]",
    "format:fix": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "lint:check": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "test": "nyc mocha --spec='src/**/*.test.ts' --spec='tests/**/*.test.ts'",
    "test:local": "yarn db:up && yarn arlocal:up && yarn test \"$@\" ; yarn arlocal:down && yarn db:down",
    "test:unit": "nyc mocha --spec='src/**/*.test.ts'",
    "test:integration": "nyc mocha --spec='tests/**/*.test.ts'",
    "test:integration:local": "yarn db:up && yarn arlocal:up && yarn test:integration \"$@\" ; yarn arlocal:down && yarn db:down",
    "test:e2e": "nyc mocha --spec='tests/e2e-*.test.ts' --timeout=30000",
    "test:e2e:aws-free": "nyc mocha --spec='tests/e2e-aws-free.int.test.ts' --timeout=30000",
    "test:e2e:ario": "nyc mocha --spec='tests/ario-optical-bridge.int.test.ts' --timeout=30000",
    "test:e2e:local": "yarn infra:up && yarn test:e2e ; yarn infra:down",
    "test:docker": "docker compose build localstack && docker compose down -v ; docker compose --env-file .env.localstack --profile test up --build test-runner --exit-code-from test-runner",
    "ci": "yarn build && yarn build:lambda && yarn test:docker",
    "build": "yarn clean && tsc --project ./tsconfig.prod.json",
    "build:lambda": "yarn build && node ./scripts/bundle-lambdas.cjs",
    "dev": "yarn clean && tsc --project ./tsconfig.prod.json -w",
    "start": "yarn node lib/index.js",
    "start:watch": "yarn nodemon -r dotenv/config -r ./src/index.ts -w src -w docs -w .env",
    "pm2:start": "yarn build && pm2 start ecosystem.config.js",
    "pm2:start:local": "yarn build && pm2 start ecosystem.config.local.js",
    "pm2:stop": "pm2 stop all",
    "pm2:delete": "pm2 delete all",
    "pm2:logs": "pm2 logs",
    "pm2:monit": "pm2 monit",
    "pm2:restart": "pm2 restart all",
    "arlocal:up": "docker compose up arlocal -d",
    "arlocal:down": "docker compose stop arlocal",
    "localstack:up": "docker compose up localstack -d",
    "localstack:down": "docker compose stop localstack",
    "db:up": "docker compose down -v ; docker compose up upload-service-pg -d && yarn db:migrate:latest",
    "db:down": "docker compose down -v",
    "infra:up": "docker compose up upload-service-pg redis redis-queues minio minio-init -d && yarn db:migrate:latest",
    "infra:down": "docker compose down",
    "infra:restart": "yarn infra:down && yarn infra:up",
    "infra:logs": "docker compose logs -f",
    "db:migrate:up": "yarn knex migrate:up $* --cwd ./src/arch/db --knexfile knexfile.ts",
    "db:migrate:down": "yarn knex migrate:down $* --cwd ./src/arch/db --knexfile knexfile.ts",
    "db:migrate:latest": "yarn knex migrate:latest --cwd ./src/arch/db --knexfile knexfile.ts",
    "db:migrate:rollback": "yarn knex migrate:rollback $* --cwd ./src/arch/db --knexfile knexfile.ts",
    "db:migrate:new": "yarn knex migrate:make $* -x ts --cwd ./src/arch/db --knexfile knexfile.ts && yarn lint:fix && yarn prettier:fix",
    "db:migrate:list": "yarn knex migrate:list --cwd ./src/arch/db --knexfile knexfile.ts"
  },
  "devDependencies": {
    "@aws-sdk/types": "^3.357.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@kyvejs/sdk": "^1.3.2",
    "@trivago/prettier-plugin-sort-imports": "^3.3.0",
    "@types/bn.js": "^5.1.0",
    "@types/chai": "^4.3.1",
    "@types/deep-equal-in-any-order": "^1.0.1",
    "@types/ioredis": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/knex": "^0.16.1",
    "@types/koa": "^2.13.4",
    "@types/koa-router": "^7.4.4",
    "@types/koa__cors": "^3.3.0",
    "@types/mocha": "^9.1.1",
    "@types/multistream": "^4.1.0",
    "@types/node": "^18.15.11",
    "@types/node-fetch": "^2.6.3",
    "@types/opossum": "^8.1.7",
    "@types/sinon": "^10.0.11",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "arlocal": "^1.1.66",
    "axios-mock-adapter": "^1.21.4",
    "chai": "^4.3.6",
    "concurrently": "^9.2.1",
    "cross-env": "^7.0.3",
    "deep-equal-in-any-order": "^2.0.6",
    "dotenv-cli": "^7.3.0",
    "esbuild": "^0.16.17",
    "eslint": "^8.15.0",
    "eslint-plugin-header": "^3.1.1",
    "husky": "^8.0.1",
    "lint-staged": "^12.5.0",
    "mocha": "^10.0.0",
    "nodemon": "^3.0.1",
    "nyc": "^15.1.0",
    "pg-query-stream": "^4.2.4",
    "pm2": "^6.0.13",
    "prettier": "^2.6.2",
    "rimraf": "^5.0.1",
    "sinon": "^14.0.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@ardrive/ardrive-promise-cache": "1.2.0-alpha.6",
    "@aws-sdk/client-s3": "3.529.0",
    "@aws-sdk/lib-storage": "3.529.0",
    "@aws-sdk/node-http-handler": "3.374.0",
    "@aws-sdk/signature-v4-crt": "3.678.0",
    "@aws-sdk/util-retry": "3.374.0",
    "@bull-board/api": "^6.13.1",
    "@bull-board/koa": "^6.13.1",
    "@cosmjs/amino": "^0.32.3",
    "@cosmjs/crypto": "^0.32.3",
    "@cosmjs/encoding": "^0.32.3",
    "@dha-team/arbundles": "^1.0.1",
    "@ethersproject/signing-key": "^5.7.0",
    "@koa/cors": "^5.0.0",
    "@opentelemetry/api": "^1.7.0",
    "@opentelemetry/auto-instrumentations-node": "^0.40.2",
    "@opentelemetry/exporter-trace-otlp-http": "^0.46.0",
    "@opentelemetry/instrumentation-aws-sdk": "^0.37.1",
    "@opentelemetry/instrumentation-pg": "^0.37.1",
    "@opentelemetry/sdk-node": "^0.46.0",
    "@smithy/node-http-handler": "^3.2.5",
    "arweave": "^1.15.1",
    "arweave-stream-tx": "^1.2.2",
    "axios": "^1.8.4",
    "axios-retry": "^3.4.0",
    "bignumber.js": "^9.1.0",
    "bs58": "^5.0.0",
    "bullmq": "^5.61.0",
    "cluster-key-slot": "^1.1.2",
    "dotenv": "^16.3.1",
    "ethers": "^6.11.1",
    "https": "^1.0.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.0",
    "knex": "^2.5.1",
    "koa": "3.0.0-alpha.3",
    "koa-router": "^13.0.1",
    "koa2-swagger-ui": "^5.8.0",
    "multistream": "4.1.0",
    "opossum": "^8.1.3",
    "p-limit": "^3.1.0",
    "pg": "^8.8.0",
    "prom-client": "^14.1.0",
    "winston": "^3.8.2",
    "yaml": "^2.3.2"
  },
  "resolutions": {
    "elliptic": "6.6.1",
    "cross-spawn": "7.0.5",
    "ip": "2.0.1",
    "tar": "6.2.1",
    "ws": "8.17.1",
    "@babel/runtime": "7.26.10"
  }
}
